        "geometry_field": "way",
        "table":
            "(
                SELECT way, \"building\" AS type
                FROM planet_osm_polygon
                WHERE building NOT IN ('', '0','false', 'no', 'church','chapel')
                ORDER BY ST_YMin(ST_Envelope(way)) DESC
            ) AS data",
