    {
      "Datasource": {
        "dbname": "osmpg_db",
        "extent": "-20037508.34,-20037508.34,20037508.34,20037508.34",
        "geometry_field": "way",
        "id": "pitch",
        "srs": "+proj=merc +a=6378137 +b=6378137 +lat_ts=0.0 +lon_0=0.0 +x_0=0.0 +y_0=0.0 +k=1.0 +units=m +nadgrids=@null +wktext +no_defs +over",
        "table":
            "(
                SELECT way, sport,
                    CASE
                        WHEN (ST_NPoints((ST_SimplifyPreserveTopology(l.way,10))) >= 5) THEN
                            (CASE WHEN (ST_MaxDistance(ST_PointN(ST_ExteriorRing((ST_SimplifyPreserveTopology(l.way,10))),1), ST_PointN(ST_ExteriorRing((ST_SimplifyPreserveTopology(l.way,10))),2)) > ST_MaxDistance(ST_PointN(ST_ExteriorRing((ST_SimplifyPreserveTopology(l.way,10))),2), ST_PointN(ST_ExteriorRing((ST_SimplifyPreserveTopology(l.way,10))),3)))
                            THEN CAST(degrees(ST_Azimuth(ST_PointN(ST_ExteriorRing((ST_SimplifyPreserveTopology(l.way,10))),2), ST_PointN(ST_ExteriorRing((ST_SimplifyPreserveTopology(l.way,10))),3))) AS VARCHAR(255))
                            ELSE CAST(degrees(ST_Azimuth(ST_PointN(ST_ExteriorRing((ST_SimplifyPreserveTopology(l.way,10))),1), ST_PointN(ST_ExteriorRing((ST_SimplifyPreserveTopology(l.way,10))),2))) AS VARCHAR(255))
                            END)
                        END AS orientation,
                    CASE
                        WHEN (ST_NPoints((ST_SimplifyPreserveTopology(l.way,10))) >= 5) THEN
                            (CASE WHEN (ST_MaxDistance(ST_PointN(ST_ExteriorRing((ST_SimplifyPreserveTopology(l.way,10))),1), ST_PointN(ST_ExteriorRing((ST_SimplifyPreserveTopology(l.way,10))),2)) > ST_MaxDistance(ST_PointN(ST_ExteriorRing((ST_SimplifyPreserveTopology(l.way,10))),2), ST_PointN(ST_ExteriorRing((ST_SimplifyPreserveTopology(l.way,10))),3)))
                            THEN CAST(ST_MaxDistance(ST_PointN(ST_ExteriorRing((ST_SimplifyPreserveTopology(l.way,10))),1), ST_PointN(ST_ExteriorRing((ST_SimplifyPreserveTopology(l.way,10))),2)) AS INTEGER)
                            ELSE CAST(ST_MaxDistance(ST_PointN(ST_ExteriorRing((ST_SimplifyPreserveTopology(l.way,10))),1), ST_PointN(ST_ExteriorRing((ST_SimplifyPreserveTopology(l.way,10))),3)) AS INTEGER)
                            END)
                        END AS length
                FROM planet_osm_polygon AS l
                WHERE leisure = 'pitch' and sport IN ('soccer', 'tennis')
            ) AS data",
        "type": "postgis"
      },
      "geometry": "point",
      "id": "pitch",
      "name": "pitch",
      "status": "on"
